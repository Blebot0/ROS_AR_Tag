<?xml version='1.0'?>

<robot name="rover" xmlns:xacro="http://www.ros.org/wiki/xacro">
<xacro:include filename="$(find mybot_description)/urdf/rover.gazebo" />
<xacro:include filename="$(find mybot_description)/urdf/materials.xacro" />
<xacro:include filename="$(find mybot_description)/urdf/macros.xacro" />


<!-- CAMERAS-->

<link name="camera">
        <collision name="collision_camera">
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                         <box size="0.1 0.1 0.1" />
                </geometry>
        </collision>


        <visual name="camera_visual">
                <origin xyz="0 0 0" rpy="0 0 0" />
                <geometry>
                        <box size="0.1 0.1 0.1" />
                </geometry>
        </visual>


        <inertial>
                <mass value="0.1" />
                <origin xyz="0 0 0.0001" rpy="0 0 0"/>
                <inertia ixx="0.0001" ixy="0.0" ixz="0.0"
                         iyy="0.0001" iyz="0.0"
                         izz="0.0001" />
        </inertial>
</link>


<link name="hokuyo">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
         <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://mybot_description/meshes/hokuyo.dae"/>
      </geometry>
    </visual>

    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
</link>


<!-- CHASSIS -->

  <link name="chassis">
    <pose>0 0 0.1 0 0 0</pose>
    <inertial>
      <mass value="10.0"/>
      <origin xyz="0.0 0 0.1" rpy=" 0 0 0"/>
      <inertia
          ixx="2.4" ixy="0" ixz="0"
          iyy="2.4" iyz="0"
          izz="2.4"
      />
    </inertial>

    <collision name='collision'>
      <geometry>
        <box size="1 1 .1"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="1 1 .1"/>
      </geometry>
    </visual>
  </link>


<!-- WHEEL LINKS -->
  <link name="left_wheel">
    <!--origin xyz="0.1 0.13 0.1" rpy="0 1.5707 1.5707"-->
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.1"/>
      </geometry>
    </collision>
    <visual name="left_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.1"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.2" h="0.1"/>
      <inertia
        ixx="0.1083" ixy="0.0" ixz="0.0"
        iyy="0.1083" iyz="0.0"
        izz="0.1083"/>
    </inertial>
  </link>


  <link name="right_wheel">
    <!--origin xyz="0.1 -0.13 0.1" rpy="0 1.5707 1.5707"/-->
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.1"/>
      </geometry>
    </collision>
    <visual name="right_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.1"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.2" h="0.1"/>
      <inertia
        ixx="0.1083" ixy="0.0" ixz="0.0"
        iyy="0.1083" iyz="0.0"
        izz="0.1083"/>
    </inertial>
  </link>


  <link name="left_fwheel">
    <!--origin xyz="-0.1 0.13 0.1" rpy="0 1.5707 1.5707"-->
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.1"/>
      </geometry>
    </collision>
    <visual name="left_wheel_fvisual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.1"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.2" h="0.1"/>
      <inertia
        ixx="0.1083" ixy="0.0" ixz="0.0"
        iyy="0.1083" iyz="0.0"
        izz="0.1083"/>
    </inertial>
  </link>


  <link name="right_fwheel">
    <!--origin xyz="-0.1 -0.13 0.1" rpy="0 1.5707 1.5707"/-->
    <collision name="collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.1"/>
      </geometry>
    </collision>
    <visual name="right_wheel_fvisual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.2" length="0.1"/>
      </geometry>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="5"/>
      <cylinder_inertia m="5" r="0.2" h="0.1"/>
      <inertia
        ixx="0.1083" ixy="0.0" ixz="0.0"
        iyy="0.1083" iyz="0.0"
        izz="0.1083"/>
    </inertial>
  </link>


<!--JOINTS -->

<joint name="camera_joint" type="fixed">
        <origin xyz="-0.45 0 0.3" rpy="0 0 3.1414" />
        <child link="camera" />
        <parent link="chassis" />
</joint>
<joint name="hokuyo_joint" type="fixed">
        <origin xyz="-0.45 0 0.25" rpy="0 0 3.1414" />
        <child link="hokuyo" />
        <parent link="chassis" />
</joint>

  <joint type="continuous" name="left_wheel_hinge">
    <origin xyz="0.25 0.565 0" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="right_wheel_hinge">
    <origin xyz="0.25 -0.565 0" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="1.0"/>
  </joint>



 <joint type="continuous" name="left_wheel_fhinge">
    <origin xyz="-0.25 0.565 0" rpy="0 0 0"/>
    <child link="left_fwheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="right_wheel_fhinge">
    <origin xyz="-0.25 -0.565 0" rpy="0 0 0"/>
    <child link="right_fwheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="100" velocity="100"/>
    <joint_properties damping="0.0" friction="1"/>
  </joint>


<!-- END -->

</robot>
